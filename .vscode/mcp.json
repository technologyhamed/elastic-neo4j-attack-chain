{
  "servers": {
    "neo4j-cypher": {
      "command": "uvx",
      "args": [
        "mcp-neo4j-cypher@0.2.3",
        "--transport",
        "stdio"
      ],
      "env": {
        "NEO4J_URI": "${env:NEO4J_URI}",
        "NEO4J_USERNAME": "${env:NEO4J_USER}",
        "NEO4J_PASSWORD": "${env:NEO4J_PASSWORD}",
        "NEO4J_DATABASE": "${env:NEO4J_DATABASE}"
      }
    }
  }
}